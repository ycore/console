{
    "name": "ycore/console",
    "description": "A super-slim collection for creating command line interfaces, with a Configuration, and Logger components",
    "keywords": ["command", "console", "config", "logger", "testing"],
    "license": "MIT",
    "authors": [
        {
            "name": "Johan Meyer",
            "email": "y-core@outlook.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/container": "^8.0",
        "psr/log": "^1.1",
        "symfony/yaml": "^5.0"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Console\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
